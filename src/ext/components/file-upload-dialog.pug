#file-upload-dialog.modal.fade(tabindex='-1', role='dialog')
  .modal-dialog
    .modal-content
      .modal-header
        button.close(type='button', data-dismiss='modal')
          span ×
          span.sr-only 关闭
        h4.modal-title 上传数据集...
      .modal-body
        form(enctype='multipart/form-data' data-bind='element:form')
          input(name='file' type='file' data-bind='file:file')

      .modal-footer
        button.btn.btn-default(type='button' data-bind='click:decline') 取消
        button.btn.btn-primary(type='button' data-bind='click:accept') 上传

